# Aigie Runtime Validation Configuration
# Copy this file to .env and fill in your API keys

# Gemini API Configuration (Required for Runtime Validation)
GEMINI_API_KEY=your_gemini_api_key_here
GOOGLE_CLOUD_PROJECT=your_project_id_here

# Optional: Gemini Location (defaults to us-central1)
GEMINI_LOCATION=us-central1

# Runtime Validation Settings
AIGIE_VALIDATION_ENABLED=true
AIGIE_VALIDATION_STRICT_MODE=false
AIGIE_VALIDATION_CACHE_TTL=300

# Performance Settings
AIGIE_VALIDATION_TIMEOUT=30
AIGIE_VALIDATION_MAX_RETRIES=3
